{
  "name": "em-gen",
  "version": "0.0.9",
  "description": "em-gen is a CLI tool for generating directories and files with emmet inspired syntax.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "precommit": "bash precommit.sh",
    "prepare": "husky install && npx husky add .husky/pre-commit",
    "lint": "eslint --ext .tsx --ignore-path .gitignore . templates/",
    "lint:fix": "eslint --ext .ts --ignore-path .gitignore --fix",
    "build": "npx tsc",
    "start": "node ./dist/index.js",
    "dev": "npx tsc && node ./dist/index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^18.14.6",
    "eslint": "^8.35.0",
    "husky": "^8.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "commander": "^10.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  }
}
