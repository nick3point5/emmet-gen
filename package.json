{
  "name": "emmet-gen",
  "version": "0.8.2",
  "description": "emmet-gen is a CLI tool for generating directories and files with emmet inspired syntax.",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "precommit": "",
    "prepare": "husky install",
    "start": "node ./src/index.js",
    "test:gh": "node ./src/index.js init && vitest",
    "test": "vitest",
    "prettier": "npx prettier --write ./src",
    "lint:fix": "eslint --ext .js --ignore-path .gitignore .  --fix",
    "lint": "eslint --ext .js --ignore-path .gitignore .",
    "semantic-release": "semantic-release"
  },
  "keywords": [
    "generate",
    "scaffolding",
    "boilerplate",
    "skeleton",
    "generate-code",
    "custom-files",
    "generator",
    "emmet",
    "cli",
    "command",
    "command-line",
    "template"
  ],
  "author": "nick3point5",
  "license": "MIT",
  "devDependencies": {
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.7",
    "@semantic-release/npm": "^9.0.2",
    "eslint": "^8.35.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-import": "^2.25.2",
    "husky": "^8.0.3",
    "semantic-release": "^20.1.1",
    "vitest": "^0.29.2"
  },
  "dependencies": {
    "commander": "^10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nick3point5/emmet-gen.git"
  },
  "bugs": {
    "url": "https://github.com/nick3point5/emmet-gen/issues"
  },
  "homepage": "https://github.com/nick3point5/emmet-gen#readme",
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  },
  "bin": "./src/index.js"
}
